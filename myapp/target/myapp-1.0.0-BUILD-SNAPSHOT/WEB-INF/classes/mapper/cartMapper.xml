<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cart">
	<insert id="saveCart">
		INSERT INTO cart (no, member_id, product_no, product_amount)
			VALUES (seq_cart.nextval, #{memberId}, #{productNo}, #{productAmount})
	</insert>
